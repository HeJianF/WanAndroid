<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SupperBannerView">
        <attr name="indicatorColor" format="color" />
        <attr name="selectIndicatorColor" format="color" />
        <attr name="indicatorSpace" format="dimension" />
        <attr name="indicatorWidth" format="dimension" />
        <attr name="indicatorHeight" format="dimension" />
        <attr name="showDuration" format="integer" />
        <attr name="onPageScroll" format="boolean" />
    </declare-styleable>

    <!-- View宽高比 -->
    <declare-styleable name="RatioByWidthView">
        <attr name="ratio" format="float" />
        <attr name="multiplyRatio" format="float" />

        <attr name="imageType">
            <enum name="origin" value="0" />
            <enum name="big" value="1" />
        </attr>
    </declare-styleable>
</resources>